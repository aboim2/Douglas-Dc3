<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>	
    <object-name>visses</object-name>
    <object-name>divers</object-name>

    <!-- Objets transparents -->
  </animation>
  
  <!-- LoD -->
  <animation>
    <type>range</type>
    <min-m>  0 </min-m>
    <max-m> 18 </max-m>
  </animation>

  <!-- Console Centrale -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Pedestal/center_console.xml</path>
    <offsets>
      <x-m> 0.01 </x-m>
      <y-m> 0.00 </y-m>
      <z-m> 0.00 </z-m>
    </offsets>
  </model>

  <!--Commandes Gear + Flaps-->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Gear-flaps/gear-flaps.xml</path>
    <offsets>
      <x-m>  0.01 </x-m>
      <y-m>  0.00 </y-m>
      <z-m>  0.05 </z-m>
    </offsets>
  </model> 

  <!--Cowl-flap command-->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Cowl-flaps/cowl-flaps.xml</path>
    <offsets>
      <x-m> 0.380 </x-m>
      <y-m> 0.825 </y-m>
      <z-m> 0.365 </z-m>
      <heading-deg> 12 </heading-deg>
      <roll-deg>    15 </roll-deg>
    </offsets>
  </model>   

  <!-- Manche -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Handle/handle.xml</path>
    <offsets>
      <x-m> -0.01 </x-m>
      <y-m>  0.00 </y-m>
      <z-m>  0.00 </z-m>
    </offsets>
  </model>

  <!-- Pédales Pilote -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Pedals/pedals.xml</path>
    <offsets>
      <x-m> -0.090 </x-m>
      <y-m> -0.430 </y-m>
      <z-m> -0.210 </z-m>
    </offsets>
  </model>

  <!-- Pédales Copilote -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Pedals/pedals.xml</path>
    <offsets>
      <x-m> -0.090 </x-m>
      <y-m>  0.430 </y-m>
      <z-m> -0.210 </z-m>
    </offsets>
  </model>

  <!--   cap -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/cap/cap.xml</path>
    <offsets>
      <x-m> -0.120 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.645 </z-m>
    </offsets>
  </model>
   
  <!--Checklist -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/checklist/checklist1.xml</path>
    <offsets>
      <x-m> -0.007 </x-m>
      <y-m> -0.693 </y-m>
      <z-m>  0.220 </z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/checklist/checklist2.xml</path>
    <offsets>
      <x-m> -0.007 </x-m>
      <y-m> -0.613 </y-m>
      <z-m>  0.220 </z-m>
    </offsets>
  </model>
  
  <!-- Pression hydraulique + Pression des trains-->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/hydgear/hydgear.xml</path>
    <offsets>
      <x-m> 0.12 </x-m>
      <y-m> 0.70 </y-m>
      <z-m> 0.40 </z-m>
      <heading-deg> -65 </heading-deg>
    </offsets>
  </model> 

  <model>
    <name>PanelTraditional</name>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/panel-traditional.xml</path>
  </model>
  
  <animation>
    <type>select</type>
    <object-name>PanelTraditional</object-name>
    <condition>
      <equals>
        <property>sim/model/config/panel</property>
        <value>traditional</value>
      </equals>
    </condition>
  </animation>

  <!-- chronometre -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/chronograph/chronograph.xml</path>
    <offsets>
      <x-m> -0.006 </x-m>
      <y-m> -0.612 </y-m>
      <z-m>  0.349 </z-m>
    </offsets>
  </model> 
  
  <!-- Manifold -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/manifold/manifold.xml</path>
    <offsets>
      <x-m> -0.016 </x-m>
      <y-m>  0.150 </y-m>
      <z-m>  0.238 </z-m>
    </offsets>
  </model>
 
  <!-- RPM -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/rpm/rpm.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m> -0.150 </y-m>
      <z-m>  0.238 </z-m>
    </offsets>
  </model>
 
  <!-- Oil pressure -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/oil/oil.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m> -0.050 </y-m>
      <z-m>  0.238 </z-m>
    </offsets>
  </model>
  
  <!-- Fuel pressure -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/fuelpress/fuelpress.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m>  0.052 </y-m>
      <z-m>  0.238 </z-m>
    </offsets>
  </model>
 
  <!-- Panel copilote -->
  
  <!-- tempérture air extérieur -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/temp/temp.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m>  0.384 </y-m>
      <z-m>  0.421 </z-m>
    </offsets>
  </model>

  <!-- oil temp -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/oiltemp/oiltemp.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m>  0.279 </y-m>
      <z-m>  0.312 </z-m>
    </offsets>
  </model>

  <!-- tempérture cylindre -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/cyl/cyl.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m>  0.384 </y-m>
      <z-m>  0.312 </z-m>
    </offsets>
  </model>

  <!-- température carb -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/carb/carb.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m>  0.484 </y-m>
      <z-m>  0.312 </z-m>
    </offsets>
  </model>

  <!-- gauges fuel -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/fuel/fuel.xml</path>
    <offsets>
      <x-m> -0.010 </x-m>
      <y-m>  0.278 </y-m>
      <z-m>  0.173 </z-m>
    </offsets>
  </model>
  
  <!-- Hyd. pressure -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/hydpress/hydpress.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m>  0.378 </y-m>
      <z-m>  0.200 </z-m>
    </offsets>
  </model>
  
  <!-- De-icing. pressure -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/deicepress/deicepress.xml</path>
    <offsets>
      <x-m> -0.015 </x-m>
      <y-m>  0.473 </y-m>
      <z-m>  0.200 </z-m>
    </offsets>
  </model>

  <!-- Warning -->
  <model>
    <path>Aircraft/Douglas-Dc3/Models/Interior/Cockpit/Panel/Instruments/warning/warning.xml</path>
    <offsets>
      <x-m> -0.01114 </x-m>
      <y-m>  0.58096 </y-m>
      <z-m>  0.17376 </z-m>
    </offsets>
  </model>

</PropertyList>
